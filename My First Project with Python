# Dictionary of items and their earnings
earnings = {
    "Bubblegum": 202,
    "Toffee": 118,
    "Ice cream": 2250,
    "Milk chocolate": 1680,
    "Doughnut": 1075,
    "Pancake": 80
}

# Print the earned amount header
print("Earned amount:")

# Print each item and its earned amount
for item, amount in earnings.items():
    print(f"{item}: ${amount}")

# Calculate total earnings
total_earnings = sum(earnings.values())

# Print total income
print(f"\nIncome: ${total_earnings:.1f}")

# Get user input for expenses
staff_expenses = int(input("Staff expenses:"))
other_expenses = int(input("Other expenses:"))

# Calculate net income
net_income = total_earnings - staff_expenses - other_expenses
print(f"Net income: ${net_income}")
